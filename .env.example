# GitAIOps Platform Environment Variables

# Application
ENVIRONMENT=development
DEBUG=True

# API Configuration
API_HOST=0.0.0.0
API_PORT=8080

# Google Cloud (replace with your values)
# GCP_PROJECT_ID=your-project-id
# GCP_LOCATION=us-central1

# GitLab (replace with your values)
# GITLAB_URL=https://gitlab.com
# GITLAB_TOKEN=your-gitlab-token
# GITLAB_WEBHOOK_SECRET=your-webhook-secret

# Database
# DATABASE_URL=postgresql://user:password@localhost/gitaiops
REDIS_URL=redis://localhost:6379

# AI/ML
# VERTEX_AI_ENDPOINT=us-central1-aiplatform.googleapis.com
# OPENROUTER_API_KEY=your-openrouter-api-key
# OPENROUTER_MODEL=anthropic/claude-opus-4

# Security
SECRET_KEY=dev-secret-key-change-in-production

# Logging
LOG_LEVEL=DEBUG

# Monitoring
ENABLE_METRICS=True
METRICS_PORT=9090
